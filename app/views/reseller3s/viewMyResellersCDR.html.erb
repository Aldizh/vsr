<table class = "list">
  <thead>
    <tr>
      <th> caller id </th>
      <th> cost (in USD) </th>
      <th> start time </th>
      <th> end time</th>
      <th> duration </th>
      <th> Call to: </th>
    </tr>
  </thead>
  <tbody>
    <% @my_cdr.each do |r| %>
     <% r.each do |c| %>
      <% number = c[:called_number] %>
      <% number.slice!("time") %>
      <tr>
      <td> <%= c[:caller_id] %></td>
      <td> <%= c[:cost] %></td>
      <td> <%= c[:call_start] %></td>
      <td> <%= c[:call_end] %> </td>
      <td> <%= c[:duration] %> s </td>
      <td> <%= number %> </td>
    </tr>
 	<% end %>
    <% end %>
  </tr>
  </tbody>
</table>